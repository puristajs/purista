{
  "name":"PURISTA API",
    "entryPointStrategy":"packages",
    "entryPoints": [
      "./packages/core",
      "./packages/httpserver",
      "./packages/amqpbridge",
      "./packages/state-store-redis",
      "./packages/k8s-sdk",
      "./packages/dapr-sdk",
      "./packages/mqttbridge"
    ],
    "plugin": ["typedoc-plugin-markdown"],
    "out": "website/src/api",
    "readme":"CHANGELOG.md",
    "hideGenerator": true,
    "includeVersion":true,
    "excludeInternal": true,
    "excludeExternals": true,
    "exclude": ["**/**/*.(test).ts","**/*+(index|.spec|.e2e|.test).ts","lib/**","/examples/**/*","node_modules","**/node_modules"]
}